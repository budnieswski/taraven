{
    "name": "budnieswski/taraven",
    "description": "Wordpress theme skeleton",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Budnieswski",
            "email": "budnieswski@gmail.com"
        }
    ],
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org"
      },
      {
        "type": "package",
        "package": {
          "name": "timber/timber",
          "version": "dev-master",
          "source": {
            "type": "git",
            "url": "https://github.com/timber/timber.git",
            "reference": "master"
          }
        }
      }
    ],
    "require": {
        "timber/timber": "dev-master",
        "matthiasmullie/minify": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-0": {
            "Taraven": "src/"
        }
    }
}